<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeiglobal.hk.repository.AuthoritiesRepository">
	
	<select id="findMemberById" parameterType="java.lang.String" resultType="com.jeiglobal.hk.domain.user.Member">
		SELECT * FROM global.Member WHERE memberId = #{memberId}
	</select>
	
	<select id="findPermissionById" parameterType="java.lang.String" resultType="com.jeiglobal.hk.domain.user.Authorities">
		SELECT * FROM global.Authorities WHERE memberId = #{memberId}
	</select>
	
</mapper>